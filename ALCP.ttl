	rdfs:domain :DirectEmploymentRow;rdfs:range xsd:dateTime.

:has-position-direct-employee rdf:type owl:DatatypeProperty;
	rdfs:domain :DirectEmploymentRow;rdfs:range xsd:string.

:has-position-start-date-direct-employee rdf:type owl:DatatypeProperty;
	rdfs:domain :DirectEmploymentRow;rdfs:range xsd:dateTime.

:has-category-direct-employee rdf:type owl:DatatypeProperty;
	rdfs:domain :DirectEmploymentRow;rdfs:range xsd:string.

:has-job-description-responsibilities-direct-employee rdf:type owl:DatatypeProperty;
