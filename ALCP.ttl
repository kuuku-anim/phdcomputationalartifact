#------------------------------------------------------------------------------
# Sections in Part1 (CompanyDetails):
#------------------------------------------------------------------------------
:CompanyDetails :has-section :ContactDetails, :OfficeAddressAndLocation, :HeadOfEntity, :ContactPerson.

:ContactDetails rdf:type :DocumentSection;
rdfs:comment "this section contains all the details concerning the companies submitting their local content plan. ".

:OfficeAddressAndLocation rdf:type :DocumentSection;
rdfs:comment "this section contains all the addres and location of the companies.".

:HeadOfEntity rdf:type :DocumentSection;
rdfs:comment "this section specifies information about the person who should have the authority to endorse this document".

:ContactPerson rdf:type :DocumentSection;
rdfs:comment "this section specifies information about the person responsible for answering questions on data provided".




#company details.

:has-registered-company-name rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:string;
rdfs:comment "this property refers to the slot containing the registered name of the company.".

:has-company-registration-number rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:string;
rdfs:comment "this property refers to the slot containing the registered number of the company.".

:has-tax-identification-number rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:string;
rdfs:comment "this property refers to the slot containing the tax identification number of the company.".

:has-date-of-incorporation rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:dateTime;
rdfs:comment "this property refers to the slot containing the date on which the company was incorporated in Ghana.".

:has-date-of-commencement-of-business rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:dateTime;
rdfs:comment "this property refers to the slot containing the date on which the company started its business activities in Ghana.".

:has-petroleum-commission-registration-permit-number rdf:type owl:DatatypeProperty;
	rdfs:domain [rdf:type owl:Class; owl:oneOf(:ContactDetails)];
	rdfs:range xsd:string;
rdfs:comment "This property refers to the slot containing the Petroleum Commission Registration Permit Number of the companies.".

