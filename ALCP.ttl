#Sections in Part :LCPlan

:LCPlan :has-section :EmploymentSubPlan, :TrainingSubPlan, :SuccessionPlan, :ResearchAndDevelopmentSubPlan, :TechnologyTransferProgram, :InsuranceAndReinsuranceServicesSubPlan, :LegalServicesSubPlan, :FinancialServicesSubPlan, :IndigenousBank.

:EmploymentSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan on companies' employment.".

:TrainingSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan of the training measures undertaken by the company.".

:SuccessionPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan of Ghanaian succeeding Expatriate in the company.".

:ResearchAndDevelopmentSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan on research and development undertaken by the company.".

:TechnologyTransferProgram rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan of technology transfer undertaken by the company.".

:InsuranceAndReinsuranceServicesSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan on companies' Insurance and Reinsurance Services obtained.".

:LegalServicesSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan on legal services employed by companies.".

:FinancialServicesSubPlan rdf:type :DocumentSection;
rdfs:comment "this section contains the sub-plan on financial services employed by companies.".

:IndigenousBank rdf:type :DocumentSection;
rdfs:comment "this section contains the details of the indigenous bank employed by the company for its services.".


#------------------------------------------------------------------------------
# Section employment sub-plan content.
#------------------------------------------------------------------------------


:DirectEmployment rdf:type :DocumentTable;
rdfs:comment "this table contains the details of the employees directly employed by a company, i.e., not employed indirectly through a third party.".

:FunctionalDistributionTable rdf:type :DocumentTable;
rdfs:comment "this table contains the number of the employees directly employed by a company I.E., the number of female, the number of males, the number of Ghanaians, etc.".

:EmploymentExpenditureTable rdf:type :DocumentTable;
rdfs:comment "this table contains the expenditure incurd in employing the direct employees.".

:ThirdPartyStaff rdf:type :DocumentTable;
rdfs:comment "this table contains the details of the third party employees employed indirectly through a third party agency or company.".

:EmploymentSubPlan :has-table :DirectEmployment, :ThirdPartyStaff.

#The individual :DirectEmployment can be associated, via the property :has-row, with one or more individuals of the class :DirectEmploymentRow. Each individual of the class
#DirectEmploymentRow is associated with the slots listed below, as encoded via datatype properties and qualifiedCardinality restrictions. Therefore, we have a *set* of values.
:DirectEmploymentRow rdf:type owl:Class;
rdfs:subClassOf :DocumentTableRow;
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-index-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-name-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-gender-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-nationality-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-department-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-employment-start-date-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:dateTime];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-position-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-position-start-date-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:dateTime];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-category-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-job-description-responsibilities-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-required-expertise-or-skills-direct-employee; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:string];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-expenditure-per-year-direct-employee-in-USD; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:decimal];
rdfs:subClassOf[rdf:type owl:Restriction; owl:onProperty :has-expenditure-per-year-direct-employee-in-GHC; owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger; owl:onDataRange xsd:decimal].

#Properties that denote the columns of DirectEmploymentRow:
:has-index-direct-employee  rdf:type owl:DatatypeProperty;
	rdfs:domain :DirectEmploymentRow; rdfs:range xsd:string.

:has-name-direct-employee rdf:type owl:DatatypeProperty;
	rdfs:domain :DirectEmploymentRow;rdfs:range xsd:string.

:has-gender-direct-employee rdf:type owl:DatatypeProperty;
